def cut(sticks):
    max = sticks[0]
    for i in sticks:
        if i > max:
            max = i
    print(max)

n = int(input())
for _ in range(n):
    sticks_n = int(input())
    sticks = input().split()
    for i in range(sticks_n):
        sticks[i] = int(sticks[i])
    cut(sticks)
